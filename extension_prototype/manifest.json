{
    "manifest_version": 3,
    "name": "Tonifier",
    "version": "1.0",
    "description": "Размечает контент страницы индикаторами тона по запросу пользователя",
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png",
    "256": "icon-256.png"
}
  }
  